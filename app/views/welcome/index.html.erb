<!DOCTYPE html>
<html>
    <head>
        <title>Hello jQuery</title>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        <script>
            var json;
            $.getJSON("data.json", function(data){
            json = data;
            });
        </script>
    </head>

    <body>
        <div>
            {{ json }}
        </div>
    </body>
</html>